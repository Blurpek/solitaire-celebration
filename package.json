{
  "name": "solitaire-celebration",
  "version": "1.1.0",
  "description": "Canvas-based solitaire card celebration animation for React and vanilla JS",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/index.iife.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.iife.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "solitaire",
    "cards",
    "confetti",
    "celebration",
    "animation",
    "canvas",
    "react",
    "javascript",
    "vanilla-js",
    "card-game",
    "react-component",
    "frontend",
    "fun",
    "physics",
    "particle-animation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blurpek/solitaire-celebration.git"
  },
  "license": "MIT"
}
